<div class="container">
  <div class="container-title">
    <h5 class="title">YOUR LISTS</h5>
  </div>
  <div class="container-list" >
        <% @lists.each do |list| %>
        <div class="card-list" style="background-image: url(<%= "https://unsplash.com/s/photos/#{list.name}" %>);">
          <%= link_to list_path(list) do %>
          <%= list.name %>
          <% end %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>



